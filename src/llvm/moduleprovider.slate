LLVM define: #ModuleProvider &parents: {LLVM Pointer}.
LLVM ModuleProvider traits atSlotNamed: #printName put: 'LLVM ModuleProvider'.

mp@(LLVM ModuleProvider traits) new [
	mp shouldNotImplement
].

"void LLVMDisposeModuleProvider(LLVMModuleProviderRef MP);"
mp@(LLVM ModuleProvider traits) dispose [
	LLVM Lib primitives LLVMDisposeModuleProvider applyTo: {mp handle}.
	mp
].
