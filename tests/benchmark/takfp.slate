
x@(Float traits) tak: y@(Float traits) with: z@(Float traits)
[
  y < x
    ifTrue: [
      ((x - 1.0) 
        tak: y with: z) 
          tak: ((y - 1.0) 
            tak: z with: x) with: ((z - 1.0) 
              tak: x with: y) ]
    ifFalse: [z]
].

n@(Integer traits) takfp
[
  (n * 3.0) tak: (n * 2.0) with: (n * 1.0)
].
